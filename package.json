{
  "name": "rehype-tree-sitter-highlight",
  "version": "1.0.2",
  "type": "module",
  "description": "highlight your code using tree-sitter-highlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinraja/rehype-tree-sitter-highlight.git"
  },
  "homepage": "https://github.com/sachinraja/rehype-tree-sitter-highlight#readme",
  "bugs": {
    "url": "https://github.com/sachinraja/rehype-tree-sitter-highlight/issues"
  },
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs",
  "keywords": [
    "tree-sitter",
    "tree-sitter-highlight",
    "rehype",
    "syntax-highlighter"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && tsup",
    "clean": "rimraf dist",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "peerDependencies": {
    "unified": "^10"
  },
  "dependencies": {
    "hast-util-to-string": "^2.0.0",
    "tree-sitter-highlight": "^1.0.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "dprint": "0.19.2",
    "esbuild-node-loader": "0.6.3",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "nano-staged": "0.5.0",
    "npm-run-all": "4.1.5",
    "rehype-parse": "8.0.3",
    "rehype-stringify": "9.0.2",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "rimraf": "3.0.2",
    "tsup": "5.11.7",
    "typescript": "4.4",
    "unified": "10.1.1",
    "vite": "2.7.6",
    "vitest": "0.0.109"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "tsup": {
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true
  }
}
